<Window x:Class="ManageMySim.Forms.Login"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ManageMySim.Forms"
        mc:Ignorable="d"
        Title="Login" Height="240.778" Width="356.944" Icon="/ManageMySim;component/Resources/Icons/MainLogo.png">
    <Grid>
        <Button x:Name="ButtonLogin" Click="ButtonLogin_Click" Content="LOGIN" HorizontalAlignment="Left" Margin="126,160,0,0" VerticalAlignment="Top" Width="120" Height="32"/>
        <TextBox x:Name="textBoxUser" HorizontalAlignment="Left" Height="23" Margin="126,42,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <Label Content="USERNAME:" HorizontalAlignment="Left" Margin="33,42,0,0" VerticalAlignment="Top"/>
        <PasswordBox x:Name="textBoxPassword" HorizontalAlignment="Left" Height="23" Margin="126,84,0,0" VerticalAlignment="Top" Width="120"/>
        <Label Content="PASSWORD:" HorizontalAlignment="Left" Margin="33,84,0,0" VerticalAlignment="Top"/>

    </Grid>
</Window>
